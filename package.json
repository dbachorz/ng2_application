{
  "name": "ng2-application",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "go": "webpack-dev-server -d --inline --progress --no-info",
    "start": "cross-env NODE_ENV=production node ./server/bin/www",
    "dev": "npm install && concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "dev:client": "webpack-dev-server -d --inline --progress --no-info",
    "dev:server": "cross-env NODE_ENV=development node ./server/bin/www",
    "build": "cross-env NODE_ENV=production webpack",
    "typings": "typings install",
    "postinstall": "npm run typings",
    "prestart": "npm run build",
    "clean": "rimraf public/",
    "test": "cross-env NODE_ENV=test mocha",
    "lint": "tslint src/**/*.ts"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "cross-env": "^1.0.8",
    "express": "~4.14.0",
    "express-jwt": "^3.4.0",
    "express-jwt-permissions": "^0.2.2",
    "jsonwebtoken": "^7.0.1",
    "md5": "^2.1.0",
    "mongoose": "^4.5.3",
    "morgan": "~1.7.0",
    "nodemailer": "^2.5.0",
    "redis": "^2.6.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "^3.0.0-beta.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "angular2-jwt": "^0.1.16",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "es6-shim": "^0.35.1",
    "file-loader": "^0.9.0",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.8.1",
    "karma": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "ng2-redux": "^3.1.1",
    "raw-loader": "^0.5.1",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^2.6.1",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.3",
    "rxjs": "5.0.0-beta.6",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-split-by-path": "0.1.0",
    "winston": "^2.2.0",
    "zone.js": "0.6.12"
  },
  "config": {
    "ghooks": {
      "post-merge": "npm install"
    }
  }
}